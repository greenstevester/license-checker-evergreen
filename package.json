{
    "name": "license-checker-evergreen",
    "description": "Extract NPM package licenses - Feature enhanced version of the original license-checker from rseidelsohn and then gladykov",
    "author": "greenstevester",
    "version": "5.0.0",
    "license": "BSD-3-Clause",
    "private": false,
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": ">=18"
    },
    "engine-strict": true,
    "keywords": [
        "license",
        "cli",
        "checker",
        "oss"
    ],
    "main": "./dist/lib/index.js",
    "types": "./dist/lib/index.d.ts",
    "bin": {
        "license-checker-evergreen": "./dist/bin/license-checker-evergreen.js"
    },
    "scripts": {
        "build": "tsc",
        "prebuild": "rimraf dist",
        "prepublishOnly": "npm run build",
        "changes": "github-changes -o evergreen -r license-checker-evergreen",
        "contrib": "node ./scripts/contrib.js",
        "pretest": "echo 'Skipping build for Jest test'",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "posttest": "echo 'Tests completed successfully'",
        "lint": "npx eslint --ext ts,js .",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "npx prettier src --check && npx prettier scripts --check",
        "prettier:fix": "npm run prettier -- --write",
        "format": "npm run prettier:fix && npm run lint:fix",
        "format:dry": "npm run prettier && npm run lint"
    },
    "preferGlobal": true,
    "bugs": {
        "url": "http://github.com/evergreen/license-checker-evergreen/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/greenstevester/license-checker-evergreen.git"
    },
    "nyc": {
        "check-coverage": true,
        "exclude": [
            "**/tests/*.js"
        ],
        "lines": 80,
        "statements": 80,
        "functions": 80,
        "branches": 80
    },
